    "@types/bcryptjs": "2.4.2",
    "@types/node": "13.1.8",
    "dotenv-cli": "3.1.0",
    "nodemon": "2.0.2",
    "prisma": "1.34.10",
    "rimraf": "3.0.0",
    "ts-node": "8.6.2",
    "typescript": "3.7.5"
  }
}
